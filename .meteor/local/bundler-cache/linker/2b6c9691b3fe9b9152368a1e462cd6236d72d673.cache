Player';\nimport Player from './../imports/ui/Player';\nimport PlayerList from './../imports/ui/PlayerList';\n\n\n\nconst renderPlayers = (playersList) => {\n    return playersList.map((player) => {\n        return <Player key={player._id} player={player}/>\n        /*return (\n\n        );*/\n    });\n};\n\n\nMeteor.startup(() => {\n    Tracker.autorun(function () {\n        let players = Players.find().fetch();\n        let title = \"Score Keep\";\n        let name = \"Geras\";\n        let jsx = (\n            <div>\n                <TitleBar title={title} subtitle=\"with meteor\"/>\n                <p>Hello {name}!</p>\n                <p>This is a second p.</p>\n                {renderPlayers(players)}\n                <AddPlayer/>\n\n            </div>\n        );\n        ReactDOM.render(jsx, document.getElementById('app'));\n    });\n\n\n\n});","import {Mongo} from 'meteor/mongo';\n\nexport const Players = new Mongo.Collection('players');","import React from 'react';\nimport {Players} from './../../imports/api/players';\n\nexport default class AddPlayer extends React.Component{\n    handleSubmit(e) {\n        e.preventDefault();\n        let playerName = e.target.playerName.value;\n        if(playerName){\n            e.target.playerName.value = '';\n            Players.insert({name: playerName, score: 0});\n        }\n    }\n\n    render(){\n        return(\n            <div>\n                <form onSubmit={this.handleSubmit}>\n                    <input type=\"text\" name=\"playerName\"/>\n                    <button>Add player</button>\n                </form>\n            </div>\n        )\n    }\n}","import React from 'react';\nimport {Players} from './../../imports/api/players'\n\nexport default class Player extends React.Component{\n    render(){\n        return(\n            <div>\n                <p>\n                    {this.props.player.name} has {this.props.player.score} point(s).\n                    <button onClick={() => Players.update({_id: this.props.player._id}, {$inc: {score: -1}})}>-1</button>\n                    <button onClick={() => Players.update({_id: this.props.player._id}, {$inc: {score: 1}})}>+1</button>\n                    <button onClick={() => Players.remove({_id: this.props.player._id})}>X</button>\n                </p>\n            </div>\n        );\n    }\n}\n\nPlayer.propTypes = {\n    player: React.PropTypes.object.isRequired\n};","import React from 'react';\n\nexport default class PlayerList extends React.Component{\n    render(){\n        return(\n            <div>\n                Player list\n            </div>\n        );\n    }\n}","import React from 'react';\n\nexport default class TitleBar extends React.Component{\n    render(){\n        return (\n            <div>\n                <h1>{this.props.title}</h1>\n                <h2>{this.props.subtitle}</h2>\n            </div>\n        );\n    }\n}\n\nTitleBar.propTypes = {\n    title: React.PropTypes.string.isRequired,\n    subtitle: React.PropTypes.string.isRequired\n}\n\nTitleBar.defaultProps = {\n\n}"]}}]