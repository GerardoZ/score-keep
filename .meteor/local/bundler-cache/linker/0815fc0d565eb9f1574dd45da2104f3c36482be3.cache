();\n        let playerName = e.target.playerName.value;\n        if(playerName){\n            e.target.playerName.value = '';\n            Players.insert({name: playerName, score: 0});\n        }\n    }\n\n    render(){\n        return(\n            <div>\n                <form onSubmit={this.handleSubmit}>\n                    <input type=\"text\" name=\"playerName\"/>\n                    <button>Add player</button>\n                </form>\n            </div>\n        )\n    }\n}","import React from 'react'\nimport TitleBar from './TitleBar';\nimport AddPlayer from './AddPlayer';\nimport PlayerList from './PlayerList';\n\nexport default class App extends React.Component{\n    render(){\n        return(\n            <div>\n                <TitleBar title={this.props.title}/>\n                <div className=\"wrapper\">\n                    <PlayerList players={this.props.players}/>\n                    <AddPlayer/>\n                </div>\n            </div>\n        );\n    }\n}\n\nApp.propsTypes = {\n    title: React.PropTypes.string.isRequired,\n    players: React.PropTypes.array.isRequired\n}","import React from 'react';\nimport {Players} from './../../imports/api/players'\n\nexport default class Player extends React.Component{\n    render(){\n        return(\n            <div>\n                <p>\n                    {this.props.player.name} has {this.props.player.score} point(s).\n                    <button onClick={() => Players.update({_id: this.props.player._id}, {$inc: {score: -1}})}>-1</button>\n                    <button onClick={() => Players.update({_id: this.props.player._id}, {$inc: {score: 1}})}>+1</button>\n                    <button onClick={() => Players.remove({_id: this.props.player._id})}>X</button>\n                </p>\n            </div>\n        );\n    }\n}\n\nPlayer.propTypes = {\n    player: React.PropTypes.object.isRequired\n};","import React from 'react';\nimport Player from './Player';\n\nexport default class PlayerList extends React.Component{\n    renderPlayers(){\n        if(this.props.players.length === 0){\n            return <p>Add your first player to get started</p>;\n        }else{\n            return this.props.players.map((player) => {\n                return <Player key={player._id} player={player}/>\n            });\n        }\n    }\n    render(){\n        return(\n            <div>\n                {this.renderPlayers()}\n            </div>\n        );\n    }\n}\n\nPlayerList.propTypes = {\n    players: React.PropTypes.array.isRequired\n}","import React from 'react';\n\nexport default class TitleBar extends React.Component{\n    renderSubtitle(){\n        if(this.props.subtitle) {\n            return <h2>{this.props.subtitle}</h2>;\n        }\n    }\n    render(){\n        return (\n            <div className=\"title-bar\">\n                <div className=\"wrapper\">\n                    <h1>{this.props.title}</h1>\n                    {this.renderSubtitle()}\n                </div>\n            </div>\n        );\n    }\n}\n\nTitleBar.propTypes = {\n    title: React.PropTypes.string.isRequired,\n    subtitle: React.PropTypes.string\n}\n\nTitleBar.defaultProps = {\n\n}"]}}]