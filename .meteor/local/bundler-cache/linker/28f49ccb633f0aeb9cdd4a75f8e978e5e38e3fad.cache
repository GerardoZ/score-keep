BACjBgB,a;iCAAe;AACX,gBAAG,KAAKf,KAAL,CAAWhC,OAAX,CAAmBgD,MAAnB,KAA8B,CAAjC,EAAmC;AAC/B,uBACI;AAAA;AAAA;AAAK,mCAAU;AAAf;AACI;AAAA;AAAA;AAAG,uCAAU;AAAb;AAAA;AAAA;AADJ,iBADJ;AAKH,aAND,MAMK;AACD,uBAAO,KAAKhB,KAAL,CAAWhC,OAAX,CAAmBe,GAAnB,CAAuB,UAACC,MAAD,EAAY;AACtC,2BAAO,oBAAC,MAAD;AAAQ,6BAAKA,OAAOyB,GAApB;AAAyB,gCAAQzB;AAAjC,sBAAP;AAEH,iBAHM,CAAP;AAIH;AACJ;;;;;yBACDT,M;0BAAQ;AACJ,mBACI;AAAA;AAAA;AACI;AAAC,4BAAD;AAAA;AAAU,iDAAyB;AAAnC;AACK,yBAAKwC,aAAL;AADL;AADJ,aADJ;AAOH;;;;;;EAvBmC5D,MAAM0C,S;;AA0B9CE,WAAWa,SAAX,GAAuB;AACnB5C,aAASb,MAAM+C,SAAN,CAAgBG,KAAhB,CAAsBD;AADZ,CAAvB,4G;;;;;;;;;;;;;;;;;;;;;;;;;AC9BAhD,OAAOsB,MAAP,CAAc;AAAC,eAAQ;AAAA,eAAIoB,QAAJ;AAAA;AAAT,CAAd;AAAsC,IAAI3C,cAAJ;AAAUC,OAAOC,KAAP,CAAaC,QAAQ,OAAR,CAAb,EAA8B;AAAA,yBAASC,CAAT,EAAW;AAACJ,gBAAMI,CAAN;AAAQ;AAApB,CAA9B,EAAoD,CAApD;;IAE3BuC,Q;;;;;;;;uBACjBmB,c;kCAAgB;AACZ,gBAAG,KAAKjB,KAAL,CAAWkB,QAAd,EAAwB;AACpB,uBAAO;AAAA;AAAA;AAAI,mCAAU;AAAd;AAAqC,yBAAKlB,KAAL,CAAWkB;AAAhD,iBAAP;AACH;AACJ;;;;;uBACD3C,M;0BAAQ;AACJ,mBACI;AAAA;AAAA;AAAK,+BAAU;AAAf;AACI;AAAA;AAAA;AAAK,mCAAU;AAAf;AACI;AAAA;AAAA;AAAK,6BAAKyB,KAAL,CAAW1B;AAAhB,qBADJ;AAEK,yBAAK2C,cAAL;AAFL;AADJ,aADJ;AAQH;;;;;;EAfiC9D,MAAM0C,S;;AAkB5CC,SAASc,SAAT,GAAqB;AACjBtC,WAAOnB,MAAM+C,SAAN,CAAgBC,MAAhB,CAAuBC,UADb;AAEjBc,cAAU/D,MAAM+C,SAAN,CAAgBC;AAFT,CAArB;AAKAL,SAASqB,YAAT,GAAwB,EAAxB,mF","file":"/app.js","sourcesContent":["import React from 'react';\nimport ReactDOM from 'react-dom';\nimport {Meteor} from 'meteor/meteor';\nimport {Players, calculatePlayerPositions} from  './../imports/api/players';\nimport {Tracker} from 'meteor/tracker';\nimport App from './../imports/ui/App';\n\n\nMeteor.startup(() => {\n    Tracker.autorun(function () {\n        let players = Players.find({},{sort:  {score: -1}}).fetch();\n        let positionedPlayers = calculatePlayerPositions(players);\n        let title = \"Score Keep\";\n        ReactDOM.render(<App title={title} players={positionedPlayers}/>, document.getElementById('app'));\n    });\n\n\n\n});","import {Mongo} from 'meteor/mongo';\nimport numeral from 'numeral';\n\nexport const Players = new Mongo.Collection('players');\n\nexport const calculatePlayerPositions = (players) => {\n    let rank = 1;\n    return players.map((player, index) => {\n        if(index !== 0 && players[index - 1].score > player.score){\n            rank++;\n        }\n        return{\n            ...player,\n            rank,\n            position: numeral(rank).format('0o')\n        }\n    });\n};","import React from 'react';\nimport {Players} from './../../imports/api/players';\n\nexport default class AddPlayer extends React.Component{\n    handleSubmit(e) {\n        e.preventDefault();\n        let playerName = e.target.playerName.value;\n        if(playerName){\n            e.target.playerName.value = '';\n            Players.insert({name: playerName, score: 0});\n        }\n    }\n\n    render(){\n        return(\n            <div className=\"item\">\n                <form className=\"form\"  onSubmit={this.handleSubmit}>\n                    <input className=\"form__input\" type=\"text\" name=\"playerName\"/>\n                    <button className=\"button\">Add player</button>\n                </form>\n            </div>\n        )\n    }\n}","import React from 'react'\nimport TitleBar from './TitleBar';\nimport AddPlayer from './AddPlayer';\nimport PlayerList from './PlayerList';\n\nexport default class App extends React.Component{\n    render(){\n        return(\n            <div>\n                <TitleBar title={this.props.title} subtitle=\"by CodeCube\"/>\n                <div className=\"wrapper\">\n                    <PlayerList players={this.props.players}/>\n                    <AddPlayer/>\n                </div>\n            </div>\n        );\n    }\n}\n\nApp.propsTypes = {\n    title: React.PropTypes.string.isRequired,\n    players: React.PropTypes.array.isRequired\n}","import React from 'react';\nimport {Players} from './../../imports/api/players'\n\nexport default class Player extends React.Component{\n    render(){\n        let itemClassName = `item`;\n\n        return(\n            <div className=\"item\">\n                <div className=\"player\">\n                    <div>\n                        <h3 className=\"player__name\">{this.props.player.name}</h3>\n                        <p className=\"player__stats\">\n                            {this.props.player.rank} {this.props.player.position} {this.props.player.score} point(s).\n                        </p>\n                    </div>\n                    <div className=\"player__actions\">\n                        <button onClick={() => Players.update({_id: this.props.player._id}, {$inc: {score: -1}})} className=\"button button--round\">-1</button>\n                        <button onClick={() => Players.update({_id: this.props.player._id}, {$inc: {score: 1}})} className=\"button button--round\">+1</button>\n                        <button onClick={() => Players.remove({_id: this.props.player._id})} className=\"button button--round\">X</button>\n                    </div>\n                </div>\n            </div>\n        );\n    }\n}\n\nPlayer.propTypes = {\n    player: React.PropTypes.object.isRequired\n};","import React from 'react';\nimport Player from './Player';\nimport FlipMove from 'react-flip-move';\n\nexport default class PlayerList extends React.Component{\n    renderPlayers(){\n        if(this.props.players.length === 0){\n            return (\n                <div className=\"item\">\n                    <p className=\"item__message\">Add your first player to get started</p>\n                </div>\n            );\n        }else{\n            return this.props.players.map((player) => {\n                return <Player key={player._id} player={player}/>;\n\n            });\n        }\n    }\n    render(){\n        return(\n            <div>\n                <FlipMove maintainContainerHeight={true}>\n                    {this.renderPlayers()}\n                </FlipMove>\n            </div>\n        );\n    }\n}\n\nPlayerList.propTypes = {\n    players: React.PropTypes.array.isRequired\n}","import React from 'react';\n\nexport default class TitleBar extends React.Component{\n    renderSubtitle(){\n        if(this.props.subtitle) {\n            return <h2 className=\"title-bar__subtitle\">{this.props.subtitle}</h2>;\n        }\n    }\n    render(){\n        return (\n            <div className=\"title-bar\">\n                <div className=\"wrapper\">\n                    <h1>{this.props.title}</h1>\n                    {this.renderSubtitle()}\n                </div>\n            </div>\n        );\n    }\n}\n\nTitleBar.propTypes = {\n    title: React.PropTypes.string.isRequired,\n    subtitle: React.PropTypes.string\n}\n\nTitleBar.defaultProps = {\n\n}"]}}]